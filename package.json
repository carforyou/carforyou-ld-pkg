{
  "name": "@carforyou/ld",
  "version": "0.0.0",
  "description": "CAR FOR YOU LaunchDarkly",
  "scripts": {
    "version": "npm run build",
    "build": "pika build",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "link": "sh ./bin/link.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carforyou/carforyou-ld-pkg.git"
  },
  "author": "CAR FOR YOU",
  "license": "MIT",
  "homepage": "https://github.com/carforyou/ld-pkg#readme",
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.1",
    "launchdarkly-js-client-sdk": "^2.16.0",
    "lodash.camelcase": "^4.3.0"
  },
  "devDependencies": {
    "@carforyou/eslint-config": "^1.1.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.0",
    "@pika/plugin-build-web": "^0.9.0",
    "@pika/plugin-ts-standard-pkg": "^0.9.0",
    "@types/jest": "^25.0.0",
    "jest": "^24.9.0",
    "react": "^16.8.6",
    "semantic-release": "^17.0.3",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "tsconfig.pika.json"
        }
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  }
}
