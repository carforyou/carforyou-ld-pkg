    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "jest": "27.4.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  },
  "sideEffects": false,
  "main": "pkg/cjs/index.js",
  "module": "pkg/esm/index.js",
  "types": "pkg/types/index.d.ts",
