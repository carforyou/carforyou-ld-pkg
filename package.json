  "license": "MIT",
  "homepage": "https://github.com/carforyou/carforyou-ld-pkg#readme",
  "peerDependencies": {
    "express": ">=4.0.0",
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@types/node": "^16.0.0",
    "hoist-non-react-statics": "^3.3.1",
    "launchdarkly-js-client-sdk": "^2.16.0",
    "launchdarkly-node-server-sdk": "^6.0.0",
