    "@testing-library/react": "12.1.5",
    "@types/jest": "29.5.5",
    "@types/react": "17.0.65",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "4.18.2",
    "jest": "29.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semantic-release": "21.1.2",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "tsconfig.pika.json"
        }
      ],
      [
